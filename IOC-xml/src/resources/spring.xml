<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean class="bean.Student" id="t">
	<property name="gender" value="Mr."></property>
	<property name="book">
		<ref bean="b"/>
	</property>
	</bean>
	<bean class ="bean.Book" id="b">
	<constructor-arg value="Pro Spring 5"></constructor-arg>
	<constructor-arg value="Iuliana Cosmina"></constructor-arg>
	</bean>
</beans>